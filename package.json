{
  "name": "money-manager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:cache-bust": "node deploy-cache-bust.js && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:pre-commit": "ng test --watch=false --browsers=ChromeHeadless",
    "build:with-tests": "npm run test:ci && ng build --configuration production",
    "cache-bust": "node deploy-cache-bust.js",
    "deploy": "npm run cache-bust && npm run build",
    "postbuild": "cp README.md dist/money-manager/README.md",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "^5.13.4",
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.6",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/platform-server": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@angular/service-worker": "^18.0.0",
    "@ngrx/effects": "^18.0.1",
    "@ngrx/entity": "^18.0.1",
    "@ngrx/store": "^18.0.1",
    "@ngrx/store-devtools": "^18.0.1",
    "angular-gridster2": "^20.1.0",
    "echarts": "^5.6.0",
    "firebase": "^11.2.0",
    "moment": "^2.30.1",
    "ngx-echarts": "^20.0.1",
    "ngx-mat-select-search": "^8.0.2",
    "ngx-papaparse": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular/cli": "^18.0.6",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^2.0.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.4.2"
  }
}
